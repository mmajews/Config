alias.pr pull --rebase
alias.p push
alias.st stash
alias.chnb checkout -b
alias.ch checkout
alias.ps push
alias.sta stash apply
alias.sts status
alias.co checkout
alias.cl clone
alias.cm commit
alias.accept-ours '!f() { git checkout --ours -- "${@:-.}"; git add -u "${@:-.}"; }; f'
alias.accept-theirs '!f() { git checkout --theirs -- "${@:-.}"; git add -u "${@:-.}"; }; f'
