[alias]
	pr = pull --rebase
	p = push
	st = stash
	chnb = checkout -b
	ch = checkout
	ps = push
	sta = stash apply
	sts = status
	co = checkout
	cl = clone
	cm = commit
	accept-ours = '!f() { git checkout --ours -- "${@:-.}"; git add -u "${@:-.}"; }; f'
	accept-theirs = '!f() { git checkout --theirs -- "${@:-.}"; git add -u "${@:-.}"; }; f'
